{
  "name": "fantasytravel/mailtester",
  "type": "library",
  "description": "Mail testing helper",
  "keywords": [
    "fantasytravel",
    "mailtester"
  ],
  "homepage": "https://github.com/fantasytravel/mailtester",
  "license": "MIT",
  "authors": [
    {
      "name": "Drakakis George",
      "email": "lolly@lollypop.gr",
      "role": "Developer"
    }
  ],
  "require": {
    "php" : "~7.2|~7.3|~7.4|~8.0|~8.1|~8.2|~8.3",
    "illuminate/contracts": "^5.0|6.*|^7.0|^8.0|^9.0",
    "illuminate/support": "^5.0|6.*|^7.0|^8.0|^9.0",
    "guzzlehttp/guzzle": "~6.0|~7.1|^7.2"
  },
  "require-dev": {
    "orchestra/testbench": "^5.0",
    "phpunit/phpunit": "^9.0",
    "squizlabs/php_codesniffer": "^3.0",
    "swiftmailer/swiftmailer": "^6.0"
  },
  "autoload": {
    "psr-4": {
      "fantasytravel\\Mailtester\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "fantasytravel\\Mailtester\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit",
    "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
    "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
